{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@quizzes/*": [
        "src/quizzes/*"
      ],
      "@scripts/*": [
        "src/scripts/*"
      ],
      "@ts/*": [
        "src/types/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}